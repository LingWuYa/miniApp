/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

text {
  font-size: 12px;
  font-family: "Microsoft YaHei";
}

.clearfix::after {
  content: "";
  width: 100%;
  height: 0;
  clear: both;
  visibility: hidden;
  display: block;
}